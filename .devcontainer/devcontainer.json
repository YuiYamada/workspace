{
	"name": "Deno DevContainer",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.yml"
	],
	"service": "deno_devcontainer",
	"workspaceFolder": "/workspace",
	"forwardPorts": [
		8080
	],
	"customizations": {
		"vscode": {
			"settings": {
				"deno.enable": true,
				"deno.lint": true,
				"deno.unstable": false
			},
			"extensions": [
				"denoland.vscode-deno"
			]
		}
	}
}
